{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": "src",
    "typeRoots": ["./node_modules/@types", "src/types"],
    "paths": {
      "@/*": ["./*"]
    },
    "outDir": "./build/",
    "rootDir": "./",
    "module": "CommonJS",
    "target": "ES5",
    "jsx": "react",
    "jsxFactory": "React.createElement"
  }
}
